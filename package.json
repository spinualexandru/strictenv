{
  "name": "dotnope",
  "version": "1.1.0",
  "author": "Alex Spinu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spinualexandru/dotnope.git"
  },
  "description": "Strict environment variable access control for Node.js",
  "main": "index.js",
  "module": "index.mjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts",
    "lib/"
  ],
  "scripts": {
    "test": "node --test 'test/**/*.js'"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "environment",
    "env",
    "security",
    "whitelist",
    "process.env"
  ],
  "license": "MIT"
}
